{
    "name": "pldin601/php-funky",
    "type": "library",
    "description": "Functional Library for PHP",
    "keywords": ["option", "lambda", "functional"],
    "homepage": "https://github.com/pldin601/php-funky",
    "authors": [
        {
            "name": "Roman Gemini (pldin601)",
            "email": "roman_gemini@ukr.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2"
    },
    "autoload": {
        "psr-4": {"Funky\\": "src/Funky/"},
